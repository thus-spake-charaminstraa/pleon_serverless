{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "sourceRoot": "apps/monolithic-app/src",
  "root": "apps/monolithic-app",
  "projects": {
    "monolithic-app": {
      "type": "application",
      "root": "apps/monolithic-app",
      "entryFile": "main",
      "sourceRoot": "apps/monolithic-app/src",
      "compilerOptions": {
        "plugins": [
          {
            "name": "@nestjs/swagger",
            "options": {
              "introspectComments": true
            }
          }
        ],
        "tsConfigPath": "apps/monolithic-app/tsconfig.app.json",
        "deleteOutDir": true
      }
    },
    "mono-lambda": {
      "type": "application",
      "root": "apps/mono-lambda",
      "entryFile": "main",
      "sourceRoot": "apps/mono-lambda/src",
      "compilerOptions": {
        "plugins": [
          {
            "name": "@nestjs/swagger",
            "options": {
              "introspectComments": true
            }
          }
        ],
        "webpack": true,
        "tsConfigPath": "apps/mono-lambda/tsconfig.app.json"
      }
    },
    "noti-lambda": {
