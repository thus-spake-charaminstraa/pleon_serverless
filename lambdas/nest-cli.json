{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "sourceRoot": "apps/monolithic-app/src",
  "root": "apps/monolithic-app",
  "projects": {
    "monolithic-app": {
      "type": "application",
      "root": "apps/monolithic-app",
      "entryFile": "main",
      "sourceRoot": "apps/monolithic-app/src",
      "compilerOptions": {
        "plugins": [
          {
            "name": "@nestjs/swagger",
            "options": {
              "introspectComments": true
            }
          }
        ],
        "tsConfigPath": "apps/monolithic-app/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "plant": {
      "type": "library",
      "root": "libs/plant",
      "entryFile": "index",
      "sourceRoot": "libs/plant/src",
      "compilerOptions": {
        "tsConfigPath": "libs/plant/tsconfig.lib.json"
      }
    },
    "auth-lambda": {
      "type": "application",
      "root": "apps/auth-lambda",
      "entryFile": "main",
      "sourceRoot": "apps/auth-lambda/src",
      "compilerOptions": {
        "plugins": [
          {
            "name": "@nestjs/swagger",
            "options": {
              "introspectComments": true
            }
          }
        ],
        "webpack": true,
        "tsConfigPath": "apps/auth-lambda/tsconfig.app.json"
      }
    },
    "user-lambda": {
      "type": "application",
      "root": "apps/user-lambda",
      "entryFile": "main",
      "sourceRoot": "apps/user-lambda/src",
      "compilerOptions": {
        "plugins": [
          {
            "name": "@nestjs/swagger",
            "options": {
              "introspectComments": true
            }
          }
        ],
        "webpack": true,
        "tsConfigPath": "apps/user-lambda/tsconfig.app.json"
      }
    },
    "plant-lambda": {
      "type": "application",
      "root": "apps/plant-lambda",
      "entryFile": "main",
      "sourceRoot": "apps/plant-lambda/src",
      "compilerOptions": {
        "plugins": [
          {
            "name": "@nestjs/swagger",
            "options": {
              "introspectComments": true
            }
          }
        ],
        "webpack": true,
        "tsConfigPath": "apps/plant-lambda/tsconfig.app.json"
      }
    }
  }
}