      "type": "application",
      "root": "apps/monolithic-app",
      "entryFile": "main",
      "sourceRoot": "apps/monolithic-app/src",
      "compilerOptions": {
        "plugins": [
          {
            "name": "@nestjs/swagger",
            "options": {
              "introspectComments": true
            }
          }
        ],
        "tsConfigPath": "apps/monolithic-app/tsconfig.app.json",
        "deleteOutDir": true
      }
    },
    "mono-app": {
